.content .slide_item image {
  width: 100%;
  height: 100%;
}
/* 返回顶部 */
.goTop {
  height: 80rpx;
  width: 80rpx;
  position: fixed;
  bottom: 80rpx;
  right: 30rpx;
}
.content_list .content_item {
  display: flex;
}
.content_list .content_item .content_url {
  width: 200rpx;
  height: 60rpx;
  vertical-align: middle;
  line-height: 60rpx;
  color: #817c7c;
  border-radius: 10rpx;
  margin: 10rpx;
  text-align: center;
  font-size: 26rpx;
  background: #e0e9f1;
}
.content_list .img_list {
  margin: 20rpx;
}
.content_list .img_list .img {
  width: 120rpx;
  height: 100rpx;
}
.content_list .im {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  margin: 60rpx 5rpx 0 78rpx;
  filter: opacity(0.6);
}
